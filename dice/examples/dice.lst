default 1
timeout -1
color light-gray/black black/green
setkey parenleft asterisk
setkey parenright parenleft
setkey equal parenright
setkey slash ampersand
setkey question underscore
setkey minus slash
setkey underscore question
setkey z y
setkey y z

title = Back to Main
configfile (nd)/tftpboot/ra3/menu.lst

title = Loader (andy) - VMWare
kernel (nd)/tftpboot/ra3/pub/rmgr -sigma0 task modname "run" task modname "andy-test"
modaddr 0x02000000
module (nd)/tftpboot/ra3/pub/fiasco -nowait -vmware -serial -nokdb -serial_esc
module (nd)/tftpboot/ra3/pub/sigma0
module (nd)/tftpboot/ra3/pub/names
module (nd)/tftpboot/ra3/pub/log
module (nd)/tftpboot/ra3/pub/dm_phys
module (nd)/tftpboot/ra3/pub/con
module (nd)/tftpboot/ra3/pub/simple_ts
module (nd)/tftpboot/ra3/pub/tftp
module (nd)/tftpboot/ra3/pub/l4exec
module (nd)/tftpboot/ra3/pub/loader (nd)/tftpboot/ra3/pub/cfg_run (nd)/tftpboot/ra3/dice_tests/cfg_andy
vbeset 0x4021

