## Section: Examine boot-time autoprobe results
## KernelOutput: /var/log/dmesg
DEBUG("autoconfigure.rules: DMESG (boot-time and kernel messages)")

DMESG('OK, FPU using',      'MATH_EMULATION', FALSE)        # Reliable
DMESG('Hmm, FPU using',     'MATH_EMULATION', FALSE)        # Reliable
DMESG('ACPI:.*found.* at', 'ACPI')                  # Not reliable
DMESG('apm: BIOS version ', 'APM')                  # Not reliable
DMESG('apm: BIOS not found.', 'APM', FALSE)         # Reliable
DMESG('parport', 'PARPORT')                         # Not reliable
DMESG('parport.: PC-style', 'PARPORT_PC')           # Not reliable
DMESG('parport.*E[CP]P', 'PARPORT_1284')            # Not reliable
DMESG('isapnp:.*detected total', 'ISAPNP')          # Reliable
DMESG('isapnp: No Plug.* device found', 'ISAPNP', FALSE)    # Reliable
DMESG('Floppy drive(s):*.fd[0-9]', 'BLK_DEV_FD')            # Not reliable
DMESG('ide[0-3]', 'BLK_DEV_IDE')                    # Not reliable
DMESG('eth[0-3]', 'NETDEVICES')                             # Not reliable
DMESG('eth[0-3]', 'NET_ETHERNET')                   # Not reliable
DMESG('matroxfb', 'MATROXFB')                               # Not reliable
DMESG('scanner.c: USB', 'USB_SCANNER')                      # Not reliable
DMESG('scsi tape', 'CHR_DEV_ST')                    # Not reliable
DMESG('ttyS[0-9]', 'SERIAL')                        # Not reliable


