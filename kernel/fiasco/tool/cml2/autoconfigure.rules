########################################################################
## Section: Processor type and flags
## KernelOutput: /proc/cpuinfo
## Note that no processor detection will take place unless ARCH is set.
## These tests are reliable; they don't depend on the current kernel config.
DEBUG("autoconfigure.rules: CPU (processor type)")

if ARCH == 'sparc':
  CPUTYPE(r'^.*:sun4u:.*:',                     'SPARC64')
  CPUTYPE(r'^.*:sun4[cdm]?:.*:',                'SPARC32')

elif ARCH == 'i386':
  # First match
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium [67]',  'M586TSC')
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium MMX',   'M586MMX')
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium Pro',   'M686')
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium II\b',  'M686')
  # Celeron Coppermine is really Pentium!!!
  CPUTYPE(r'^GenuineIntel:.*:.*:.*Coppermine',  'MPENTIUMIII')
  CPUTYPE(r'^GenuineIntel:.*:.*:Celeron',       'M686')
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium III',   'MPENTIUMIII')
  CPUTYPE(r'^GenuineIntel:.*:.*:Pentium 4',     'MPENTIUM4')
  CPUTYPE(r'^GenuineIntel:4:.*:',               'M486')
  CPUTYPE(r'^GenuineIntel:5:[01237]:',          'M586TSC')
  CPUTYPE(r'^GenuineIntel:5:[48]:',             'M586MMX')
  CPUTYPE(r'^GenuineIntel:6:[01356]:',          'M686')
  CPUTYPE(r'^GenuineIntel:6:[789]:',            'MPENTIUMIII')
  CPUTYPE(r'^GenuineIntel:6:1[1]:',             'MPENTIUMIII')

  CPUTYPE(r'^AuthenticAMD:.*:.*:K5',            'M586')
  CPUTYPE(r'^AuthenticAMD:.*:.*:K6',            'MK6')
  CPUTYPE(r'^AuthenticAMD:.*:.*:Athlon',        'MK7')
  CPUTYPE(r'^AuthenticAMD:4:.*:',               'M486')
  CPUTYPE(r'^AuthenticAMD:5:[0123]:',           'M586')
  CPUTYPE(r'^AuthenticAMD:5:[89]:',             'MK6')
  CPUTYPE(r'^AuthenticAMD:5:1[01]:',            'MK6')
  CPUTYPE(r'^AuthenticAMD:6:[01246]:',          'MK7')

  CPUTYPE(r'^UMC:4:[12]:',                      'M486')
  CPUTYPE(r'^NexGenDriven:.*:.*:Nx586',         'M386')
  CPUTYPE(r'^NexGenDriven:5:0:',                'M386')

  CPUTYPE(r'^TransmetaCPU:.*:.*:',              'MCRUSOE')
  CPUTYPE(r'^GenuineTMx86:.*:.*:',              'MCRUSOE')

  CPUTYPE(r'^CentaurHauls:.*:.*:WinChip C6',    'MWINCHIPC6')
  CPUTYPE(r'^CentaurHauls:.*:.*:WinChip 2\b',   'MWINCHIP2')
  CPUTYPE(r'^CentaurHauls:.*:.*:WinChip 2[AB]', 'MWINCHIP2A')
  CPUTYPE(r'^CentaurHauls:.*:.*:WinChip [34]',  'MWINCHIP2A')
  CPUTYPE(r'^CentaurHauls:5:4:',                'MWINCHIPC6')

  # default value
  CPUTYPE(r'^.*:.*:.*:',                        'M386')

# Tests that are not platform-specific
CPUINFO(r"flags.*(mtrr)",   'MTRR',           PRESENT, y)
CPUINFO(r"fpu\s*:.*(yes)",  'MATH_EMULATION', PRESENT, n)
CPUINFO(r"fpu\s*:.*(no)",   'MATH_EMULATION', PRESENT, y)


