
$(SIGMA0): $(SIGMA0).image
	@echo "  ... Building sigma0 ELF image"
	$(LINK_MESSAGE) 
	$(VERBOSE)$(OBJCOPY) -j ".text" -j ".data" -j ".bss" -S $< $@ 

$(SIGMA0).image: $(srcdir)/sigma0.arm.ld $(OBJ_SIGMA0) $(CXXLIB)  $(MINILIBC)
	@echo "  ... Building sigma0 image"
	$(LINK_MESSAGE) 
	$(VERBOSE)$(LD) -T $^ $(LIBGCC) -o $@ 

clean-SIGMA0:
		rm -f sigma0

cleanall-SIGMA0:
		rm -f sigma0
